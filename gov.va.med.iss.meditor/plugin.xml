<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="gov.va.med.iss.meditor.treemresources">
         <iterate
               ifEmpty="false"
               operator="and">
            <or>
               <instanceof
                     value="org.eclipse.core.resources.IFolder">
               </instanceof>
               <instanceof
                     value="org.eclipse.core.resources.IProject">
               </instanceof>
               <and>
                  <instanceof
                        value="org.eclipse.core.resources.IFile">
                  </instanceof>
                  <test
                        property="org.eclipse.core.resources.extension"
                        value="m">
                  </test>
               </and>
            </or>
         </iterate>
      </definition>
      <definition
            id="gov.va.med.iss.meditor.treemresourceswithfolders">
         <and>
            <iterate
                  ifEmpty="false"
                  operator="or">
               <or>
                  <instanceof
                        value="org.eclipse.core.resources.IProject">
                  </instanceof>
                  <instanceof
                        value="org.eclipse.core.resources.IFolder">
                  </instanceof>
               </or>
            </iterate>
            <reference
                  definitionId="gov.va.med.iss.meditor.treemresources">
            </reference>
         </and>
      </definition>
      <definition
            id="gov.va.med.iss.meditor.onlyfolder">
         <and>
            <iterate
                  ifEmpty="false"
                  operator="and">
               <or>
                  <instanceof
                        value="org.eclipse.core.resources.IFolder">
                  </instanceof>
                  <instanceof
                        value="org.eclipse.core.resources.IProject">
                  </instanceof>
               </or>
            </iterate>
            <count
                  value="1">
            </count>
         </and>
      </definition>
   </extension>

   <extension
         id="gov.va.med.iss.meditor.meditor"
         name="gov.va.med.iss.meditor.meditor"
         point="org.eclipse.ui.editors">
      <editor
            class="gov.va.med.iss.meditor.editors.MEditor"
            icon="icons/mfile.gif"
            contributorClass="gov.va.med.iss.meditor.editors.MEditorContributor"
            name="MEditor"
            id="gov.va.med.iss.meditor.editors.MEditor"
            extensions="m"/>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            name="M-Editor"
            category="gov.va.med.iss.connection.preferences.VistaPreferencePage"
            class="gov.va.med.iss.meditor.preferences.MEditorPreferencesPage"
            id="gov.va.med.iss.meditor.preferences.MEditorPreferencesPage">
      </page>
    </extension>
   <extension
         id="gov.va.med.iss.meditor.preferences.meditorPropertyPage1"
         name="M-Editor"
         point="org.eclipse.ui.propertyPages">
      <page
            class="gov.va.med.iss.meditor.preferences.MEditorPropertyPage1"
            icon="icons/meditor.gif"
            id="gov.va.med.iss.meditor.page1"
            name="M-Editor"
            nameFilter="*.m">
         <enabledWhen>
            <instanceof
                  value="org.eclipse.core.resources.IFile">
            </instanceof>
         </enabledWhen>
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.navigator.ProjectExplorer#PopupMenu?after=additions">
         <menu
               icon="icons/meditor.gif"
               label="M Server Util">
            <command
                  commandId="gov.va.med.iss.meditor.command.saveRoutines"
                  label="Save Routines"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <reference
                        definitionId="gov.va.med.iss.meditor.treemresources">
                  </reference>
               </visibleWhen>
            </command>
            <command
                  commandId="gov.va.med.iss.meditor.command.saveRoutines"
                  label="Save Routines in Namespace"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <reference
                        definitionId="gov.va.med.iss.meditor.treemresourceswithfolders">
                  </reference>
               </visibleWhen>
               <parameter
                     name="gov.va.med.iss.meditor.command.saveRoutines.specifyNamespace"
                     value="true">
               </parameter>
            </command>
            <command
                  commandId="gov.va.med.iss.meditor.command.loadExistingRoutines"
                  label="Load Routines"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <reference
                        definitionId="gov.va.med.iss.meditor.treemresources">
                  </reference>
               </visibleWhen>
            </command>
            <command
                  commandId="gov.va.med.iss.meditor.command.loadRoutinesIntoDirectory"
                  label="Load Routines in Namespace"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <reference
                        definitionId="gov.va.med.iss.meditor.onlyfolder">
                  </reference>
               </visibleWhen>
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:gov.va.med.iss.connection.vista?after=additions">
         <command
               commandId="gov.va.med.iss.meditor.command.loadMRoutine"
               icon="icons/meditor.gif"
               label="&amp;Load M Routine"
               style="push">
         </command>
         <separator
               name="gov.va.med.iss.meditor.seperator1"
               visible="true">
         </separator>
         <command
               commandId="gov.va.med.iss.meditor.command.reportGlobalDirectory"
               icon="icons/meditor.gif"
               label="Global Directory"
               style="push">
         </command>
         <command
               commandId="gov.va.med.iss.meditor.command.reportGlobalListing"
               icon="icons/meditor.gif"
               label="Global Listing"
               style="push">
         </command>
         <command
               commandId="gov.va.med.iss.meditor.command.reportRoutineDirectory"
               icon="icons/meditor.gif"
               label="Routine Directory"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="gov.va.med.iss.meditor.toolbar1">
            <command
                  commandId="gov.va.med.iss.meditor.command.loadMRoutine"
                  icon="icons/meditor.gif"
                  label="Load M Routine"
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="gov.va.med.iss.meditor.command.SaveRoutines"
            id="gov.va.med.iss.meditor.command.saveRoutines"
            name="Save Routines">
         <commandParameter
               id="gov.va.med.iss.meditor.command.saveRoutines.specifyNamespace"
               name="Specify Namespace"
               optional="false">
         </commandParameter>
      </command>
      <command
            defaultHandler="gov.va.med.iss.meditor.command.LoadExistingRoutines"
            id="gov.va.med.iss.meditor.command.loadExistingRoutines"
            name="Load Existing Routines">
      </command>
      <command
            defaultHandler="gov.va.med.iss.meditor.command.LoadRoutinesIntoDirectory"
            id="gov.va.med.iss.meditor.command.loadRoutinesIntoDirectory"
            name="Load Routines Into Directory">
      </command>
      <command
            defaultHandler="gov.va.med.iss.meditor.command.LoadMRoutine"
            id="gov.va.med.iss.meditor.command.loadMRoutine"
            name="Load M Routine">
      </command>
      <command
            defaultHandler="gov.va.med.iss.meditor.command.ReportGlobalDirectory"
            id="gov.va.med.iss.meditor.command.reportGlobalDirectory"
            name="Global Directory">
      </command>
      <command
            defaultHandler="gov.va.med.iss.meditor.command.ReportGlobalListing"
            id="gov.va.med.iss.meditor.command.reportGlobalListing"
            name="Global Listing">
      </command>
      <command
            defaultHandler="gov.va.med.iss.meditor.command.ReportRoutineDirectory"
            id="gov.va.med.iss.meditor.command.reportRoutineDirectory"
            name="Routine Directory">
      </command>
   </extension>
</plugin>
